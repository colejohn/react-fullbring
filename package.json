{
	"name": "react-fullbring",
	"version": "1.0.0",
	"description": "Simple react graphql mongodb fullstack development",
	"main": "index.js",
	"scripts": {
		"server": "nodemon --exec babel-node ./server/index.js",
		"client": "npm start --prefix client",
		"start": "concurrently \"npm run client\" --kill-others-on-fail \"npm run server\""
	},
	"keywords": [ "react", "graphql", "mongodb", "apollo" ],
	"author": "cole",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.6",
		"@babel/node": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.6",
		"@babel/plugin-transform-async-to-generator": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.9.6",
		"@babel/preset-env": "^7.9.6",
		"@babel/preset-react": "^7.9.4",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"concurrently": "^5.2.0"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"express-graphql": "^0.9.0",
		"graphql": "^15.0.0",
		"graphql-subscriptions": "^1.1.0",
		"graphql-tools": "^5.0.0",
		"graphql-yoga": "^1.18.3",
		"merge-graphql-schemas": "^1.7.8",
		"mongoose": "^5.9.15",
		"nodemon": "^2.0.4"
	}
}
